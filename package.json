{
  "name": "electron-react-ts",
  "version": "1.0.0",
  "description": "Electron + React + Typescript + webpack5 + antd5",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack5",
    "antd"
  ],
  "main": "electron/electron.js",
  "scripts": {
    "start:react": "cross-env NODE_ENV=development APP_ENV=dev webpack-dev-server -c config/webpack/webpack.dev.js",
    "start:electron": "cross-env NODE_ENV=development APP_ENV=dev node ./config/electron/build_dev.js",
    "start:both": "concurrently \"npm run start:react\" \"wait-on http://localhost:9090 && npm run start:electron \"",
    "build:mac": "node -e \"require('./config/electron/build_prod.js').build('--mac')\"",
    "build:win": "node -e \"require('./config/electron/build_prod.js').build('--win')\"",
    "build:react-dev": "cross-env NODE_ENV=production APP_ENV=dev webpack -c config/webpack/webpack.prod.js",
    "build:react-analy": "cross-env NODE_ENV=production APP_ENV=prod webpack -c config/webpack/webpack.analy.js",
    "build:react-prod": "cross-env NODE_ENV=production APP_ENV=prod webpack -c config/webpack/webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YDMua/electron-react-ts.git"
  },
  "author": "蚂蚁",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YDMua/electron-react-ts/issues"
  },
  "homepage": "https://github.com/YDMua/electron-react-ts#readme",
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "antd": "^5.6.4",
    "classnames": "^2.3.2",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@swc/core": "^1.3.68",
    "@swc/helpers": "^0.5.1",
    "@types/electron": "^1.6.10",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.31.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "electronmon": "^2.0.2",
    "esbuild": "^0.18.11",
    "esbuild-loader": "^3.0.1",
    "fs-extra": "^11.1.1",
    "glob-all": "^3.3.1",
    "html-webpack-plugin": "^5.5.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "purgecss-webpack-plugin": "^5.0.0",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "thread-loader": "^4.0.2",
    "typescript": "^5.1.6",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
